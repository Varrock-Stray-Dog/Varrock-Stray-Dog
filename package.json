{
    "name": "varrock-stray-dog",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "postinstall": "ngcc --properties es2020 browser module main",
        "lint": "nx workspace-lint && ng lint",
        "prebuild": "rm -rf dist && yarn prisma:generate",
        "build": "nx build",
        "build:web": "yarn build web -c production",
        "build:bot": "yarn build bot -c production",
        "build:api": "yarn build api -c production",
        "build:sync": "yarn build item-sync -c production",
        "prisma:studio": "yarn prisma studio",
        "prisma:migrate:save": "yarn prisma migrate dev",
        "prisma:migrate:deploy": "yarn prisma migrate deploy",
        "prisma:generate": "prisma generate",
        "prisma:complete": "yarn prisma:migrate:save && yarn prisma:migrate:deploy && yarn prisma:generate",
        "docker:prestart": "yarn && yarn prisma:generate",
        "docker:start:api": "yarn docker:prestart && nx run api:serve",
        "docker:start:bot": "yarn docker:prestart && nx run bot:serve",
        "docker:start:web": "yarn && nx run web:serve --host 0.0.0.0 --disable-host-check",
        "prod:api": "node ./dist/apps/api/main.js",
        "prod:bot": "node ./dist/apps/bot/main.js",
        "sync": "node ./dist/packages/item-sync/main.js sync"
    },
    "private": true,
    "resolutions": {
        "fastify": "^3"
    },
